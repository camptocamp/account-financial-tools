<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="purchase_order_line_tree_specific" model="ir.ui.view">
    <field name="name">purchase.order.line.tree.specific</field>
    <field name="model">purchase.order</field>
    <field name="inherit_id" ref="purchase.purchase_order_form"/>
    <field name="arch" type="xml">
      <xpath
        expr="//notebook/page/field[@name='order_line']/tree/field[@name='name']"
        position="after">
        <field name="req_sn_supplier"/>
      </xpath>
    </field>
  </record>

</odoo>
